{
  "name": "holo-pomodoro",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "migrate": "per-env",
    "migrate:development": "dotenv -- ts-node ./src/cli/migrate.ts",
    "migrate:production": "node ./lib/cli/migrate.js",
    "prepareVtuberProfiles": "per-env",
    "prepareVtuberProfiles:development": "dotenv -- ts-node ./src/cli/prepareVtuberProfiles.tsx",
    "prepareVtuberProfiles:production": "node ./lib/cli/prepareVtuberProfiles.js",
    "build": "npm run clean && npm run build:src && npm run build:webview",
    "build:src": "tsc",
    "build:webview": "dotenv -- next build ./webview",
    "start": "per-env",
    "start:development": "dotenv -- ts-node ./src/index.ts",
    "start:production": "node ./lib/index.js",
    "dev": "dotenv -- ts-node ./src/cli/dev.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@devexpress/dx-react-core": "^2.7.6",
    "@devexpress/dx-react-scheduler": "^2.7.6",
    "@devexpress/dx-react-scheduler-material-ui": "^2.7.6",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@machinat/core": "0.6.0-beta.1",
    "@machinat/dev-tools": "0.6.0-beta.0",
    "@machinat/dialogflow": "0.6.0-beta.0",
    "@machinat/http": "0.6.0-beta.0",
    "@machinat/line": "0.6.0-beta.4",
    "@machinat/messenger": "0.6.0-beta.5",
    "@machinat/per-env": "^1.1.0",
    "@machinat/redis-state": "0.6.0-beta.0",
    "@machinat/script": "0.6.0-beta.0",
    "@machinat/stream": "0.6.0-beta.0",
    "@machinat/telegram": "0.6.0-beta.5",
    "@machinat/twitter": "0.6.0-beta.5",
    "@machinat/webview": "0.6.0-beta.5",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.0.0",
    "commander": "^7.2.0",
    "next": "^12.0.8",
    "node-fetch": "^2.6.6",
    "ordinal": "^1.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-vis": "^1.11.7",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "dotenv-cli": "^4.1.1",
    "localtunnel": "^2.0.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5"
  }
}
